{
	"type": "module",
	"name": "@encode42/nbs.js",
	"description": "Robust API for reading and writing OpenNBS files.",
	"version": "6.0.0-beta",
	"author": {
		"name": "encode42",
		"email": "me@encode42.dev",
		"url": "https://encode42.dev"
	},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/encode42"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/encode42/nbs.js.git"
	},
	"homepage": "https://encode42.github.io/nbs.js/",
	"license": "MIT",
	"keywords": ["api", "minecraft", "nbs", "noteblock", "onbs"],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist", "changelog.md"],
	"sideEffects": false,
	"scripts": {
		"test": "biome format . --write && biome lint . && bun test",
		"build": "bun build src/index.ts --minify --sourcemap=linked --outdir=dist && tsc --emitDeclarationOnly --outFile dist/index.js",
		"build:publish": "bun run build && bun publish",
		"build:docs": "typedoc"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.1.10",
	"devDependencies": {
		"@biomejs/biome": "^1.8.2",
		"@types/bun": "^1.1.5",
		"bun-plugin-dts": "^0.2.3",
		"typedoc": "^0.25.13",
		"typedoc-material-theme": "^1.0.3",
		"typedoc-plugin-include-example": "^1.2.0",
		"typedoc-plugin-mdn-links": "^3.2.1",
		"typescript": "^5.5.2"
	}
}
